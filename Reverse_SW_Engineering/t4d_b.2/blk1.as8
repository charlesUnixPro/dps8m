" test TRC with carry set

test_l52:
	ldi	IR_carry_bit	" set the carry bit, clear zero and negative
	trc	l55
	idis	test_l52	" Fail: TRC with carry set

" test TZE with zero clear

l55:
	tze	* + 2
	tra	* + 2	" Success
	idis	0000052	" Fail: TZE with carry set

" test TPL with negative clear

	tpl	* + 2
	idis	test_l52	" Fail: TZE with carry set

" test TPL with negative set

	ldi	IR_negative_bit	" Set Negative bit
	tpl	* + 2
	tra	* + 2	" Success
	idis	test_l52	" Fail: TPL with negative set

" test TRC with carry clear

	trc	* + 2
	tra	* + 2	" Success
	idis	0000052	" Fail: TRC with negative set

" test TZE with zero set

	ldi	IR_zero_bit	" Set zero only
	tze	* + 2
	idis	test_l52	" Fail: TZE with zero set

" test TPL with negative clear

	tpl	* + 2
	idis	test_l52	" Fail: TPL with zero set

" test TZE with all clear

	ldi	C_zero	" Clear all bits
	tze	* + 2
	tra	* + 2	" Success
	idis	test_l52	" Fail: TZE with all clear

