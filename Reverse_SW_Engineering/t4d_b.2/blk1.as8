""
""    Test ADA C1 + C2 = 0 sets IR
""

	macro	test_ada_C1_C2_IR "test_label,C1, C2
#1:	lda	#2	"C1
	tze	* + 3
	tpl	* + 3
	tra	* + 3
	idis	#1	" Fail TZE
	idis	#1	" Fail TPL
	ada	#3	" C_2
	tpl	* + 2
	idis	#1	" Fail TPL
	trc	* + 2
	idis	#1	" Fail TRC
	tze	* + 2
	idis	#1	" Fail TZE
	endm


" test_l102: C_400000000000 + C_400000000000 


test_l102:
        " test overflow mask?
	ldi	IR_ovmask_bit	" Set overflow mask
	tra	* + 2
	idis			" Fail ???

	lda	C_400000000000
	tpl	* + 2
	tra	* + 2	" Success
	idis	test_l102	" Fail: TPL with C_400000000000
	tze	* + 2
	tra	* + 2	" Success
	idis	test_l102	" Fail: TZE with C_400000000000

	ada	C_400000000000
	trc	* + 2
	idis	test_l102	" Fail: TRC with 400000000000+400000000000
	tze	l121
	idis	test_l102	" Fail: TZE with 400000000000+400000000000


	test_ada_C1_C2_IR	l121, C_600000000000, C_200000000000

