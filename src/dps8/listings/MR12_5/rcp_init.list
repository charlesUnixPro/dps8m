      528 INIT_SPECIAL:
      529      procedure;
      530 
      531 /*	This procedure is called to set up a special type device for
      532*   *	the tape or disk controller.
      533**/
      534 
      535 	save_dtypex = dtypex;			/* Save real device type index. */
      536 	dtypex = special_dtypex;			/* Get device type index for special devices. */
      537 	dtype_ptr = addr (rcpd.dtype (dtypex));		/* Pointer to special device info. */
      538 	num_qualifiers = 0;				/* No characteristics known about special devices. */
      539 
      540 	call INIT_DEVICE;				/* Define a device. */
      541 
      542 	dtypex = save_dtypex;			/* Restore original information. */
      543 	dtype_ptr = addr (rcpd.dtype (dtypex));		/* Reset device type pointer */
      544 	num_qualifiers = dtype_num_qualifiers (dtypex);
      545 
      546      end INIT_SPECIAL;
