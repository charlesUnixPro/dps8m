      653 		     if config_data_$tape_drive_model_names.names (i).model = model
      654 		     then return (config_data_$tape_drive_model_names.names (i).device_0_valid);
      655 		end;
      656 		return ("0"b);
      657 		end;
      658 	end;
      659 	return ("0"b);
