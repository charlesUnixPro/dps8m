      636 IS_FIPS_DEVICE:
      637      proc () returns (bit (1));
      638 
      639 /*  This proc will look up the model in config_data_ for the
      640*   dtypex checking for  device_0_valid */
      641 
      642 dcl  i fixed bin;
      643 dcl  groupx fixed bin;
