      619 DEVICE_DELETED:
      620      procedure (a_name) returns (bit (1));
      621 
      622 /* local vars */
      623 dcl  a_name char (*);
      624 dcl  (found, deleted) bit (1) init ("0"b);
      625 dcl  i fixed bin;
      626 
      627 	do i = 1 to device_table.n_devices while (^found);
      628 	     if device_entry (i).name = a_name then found = "1"b;
      629 	end;
      630 
      631 	if ^device_entry (i - 1).flags.configured then deleted = "1"b;
      632 
      633 	return (deleted);
      634      end DEVICE_DELETED;
