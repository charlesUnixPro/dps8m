      422       call rldr_label_$close (rldr_datap, pvindex, code);
      423       if code ^= 0 then
      424         call rldr_report_$error_output (rldr_datap, code, myname, "Error closing volume label for pv ^a",
      425 	   rldr_data_.pvname (pvindex));
