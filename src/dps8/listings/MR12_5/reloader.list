      417       call rldr_volume_map_$close (rldr_datap, pvindex, code);
      418       if code ^= 0 then
      419         call rldr_report_$error_output (rldr_datap, code, myname, "Error closing volume map for pv ^a",
      420 	   rldr_data_.pvname (pvindex));
