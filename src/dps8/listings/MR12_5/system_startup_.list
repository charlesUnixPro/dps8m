      825 try_config_again:
      826 	if config_deck.cards (1).word = ZERO_CARD_WORD
      827 	then do;					/* It's empty already */
      828 		call hphcs_$syserr (CRASH, "^a: The config deck is empty.", ME);
      829 		call hphcs_$call_bce;
      830 		go to try_config_again;		/* perhaps fixed up in bce */
      831 	     end;
      832 
      833 	do idx = 1 to config_max_cards;		/* Otherwise, look for the first free card */
      834 	     if config_deck.cards (idx).word = FREE_CARD_WORD
      835 	     then goto FOUND_FREE_CARD;
      836 	end;
