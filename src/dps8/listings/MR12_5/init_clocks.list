       40 /* Constants */
       41 
       42 	declare me		 char (11) init ("init_clocks") static options (constant);
       43 	declare time_format		 char (41) int static options (constant)
       44 				 init ("^dn, ^mn ^Z9dm, ^9999yc ^Z9Hd:^MH:^SM ^za");
