     1083 	then do;
     1084 	     old_RS_len = old_RS_len + quote_len;
     1085 	     quote_len = verify (substr (RS, old_RS_len + 1), """") - 1;
     1086 	     if quote_len < 0 then quote_len = RS_len - old_RS_len;
     1087 	     old_RS_len = old_RS_len + quote_len;
     1088 	     if mod (quote_len, twoL) = 0
     1089 	     then do while (mod (quote_len, 2 * twoL) ^= 0);
