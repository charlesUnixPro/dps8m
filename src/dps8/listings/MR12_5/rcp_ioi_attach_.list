       34 /*	*/
       35 	rcse_ptr = arg_rcse_ptr;		/* Copy argument. */
       36 
       37 	call admin_gate_$ioi_attach (rcse.ioi_index, rcse.device_name, rcse.event_id, (rcse.flags.priv), ecode);
       38 	arg_ecode = ecode;
       39 	if   ecode ^= 0			/* If attachment failed don't output messages. */
       40 	     then return;
       41 
       42 	call admin_gate_$syserr (0, "RCP: Attached ^a for ^a", rcse.device_name, rcse.group_id);
       43 	call rcp_comment_ (rcse_ptr);		/* Type out any user comment. */
