       13 rcp_ioi_attach_:  procedure (arg_rcse_ptr, arg_ecode);
       14 
       15 /*	This program is an internal RCP interface.
       16**	Created on 07/14/75 by Bill Silver.
       17**
       18**	This program will call IOI to attach the device specified in the RCSE.
       19**	If the attachment is successful an attachment message will be output
       20**	to the operator.  If the caller has specified a comment, a note message
       21**	will also be output to the operator.
       22**/
       23 
       24 dcl	arg_ecode		fixed bin(35);	/* (O) Return error_table_ code. */
       25 dcl	arg_rcse_ptr	ptr;		/* (I) Pointer to the RCSE that specifies the device. */
       26 
       27 dcl	ecode		fixed bin(35);	/* error_table_ code. */
       28 
       29 dcl	admin_gate_$ioi_attach	entry  (fixed bin, char(*) aligned, fixed bin(71), bit(1) aligned, fixed bin(35));
       30 dcl	admin_gate_$syserr		entry  options(variable);
       31 dcl	rcp_comment_		entry  (ptr);
