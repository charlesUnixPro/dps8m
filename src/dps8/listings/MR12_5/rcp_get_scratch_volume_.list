      131 	     resource_descriptions.version_no = resource_desc_version_1;
      132 	     resource_descriptions.n_items = 1;
      133 	     resource_descriptions.item (1).type = VOLUME_TYPE (rcse.dtypex);
      134 	     goto MAKE_ATTRS (rcse.dtypex);
      135 
      136 
      137 MAKE_ATTRS (1):					/* TAPE VOL */
      138 
      139 	     if rcse.qualifiers (1) = 9
      140 	     then attrs = "track=9";
      141 	     else attrs = "track=7";
      142 
      143 	     do i = 1 to hbound (DEN_NAME, 1);
      144 		if substr (unspec (rcse.qualifiers (2)), i, 1)
      145 		then attrs = attrs || DEN_NAME (i);
      146 	     end;
      147 	     goto MADE_ATTRS;
      148 
      149 
      150 MAKE_ATTRS (2):					/* DISK VOL */
      151 
      152 	     model_pic = rcse.model;
      153 	     attrs = "model=" || ltrim (model_pic) || ",use=io";
      154 	     goto MADE_ATTRS;
