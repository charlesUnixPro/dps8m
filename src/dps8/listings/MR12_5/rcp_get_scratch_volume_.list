      158 MAKE_ATTRS (4):					/* NOT USED */
      159 MAKE_ATTRS (5):					/* NOT USED */
      160 MAKE_ATTRS (6):					/* NOT USED */
      161 MAKE_ATTRS (7):					/* NOT USED */
      162 MAKE_ATTRS (8):					/* NOT USED */
      163 
      164 	     attrs = "";
      165 
      166 
      167 MADE_ATTRS:
      168 
      169 	     if attrs = ""
      170 	     then resource_descriptions.item (1).desired_attributes (*) = "0"b;
      171 	     else do;
      172 		     call cv_rcp_attributes_$from_string_rel ((VOLUME_TYPE (rcse.dtypex)),
      173 			resource_descriptions.item (1).desired_attributes (*), attrs, code);
      174 		     if code ^= 0
      175 		     then return;
      176 		     resource_descriptions.item (1).given.desired_attributes = "1"b;
      177 		end;
      178 
      179 	     call rcprm_find_resource_$reserve (resource_desc_ptr, (rcs.acs_directory), null (), code);
      180 	     if code = error_table_$action_not_performed then
      181 		code = resource_descriptions.item (1).status_code;
      182 	     if code ^= 0 then return;
      183 
      184 	     rsc_name = resource_descriptions.item (1).name;
      185 	     access = resource_descriptions.item (1).rew;
      186 
      187 	end;
      188 
      189 	return;
