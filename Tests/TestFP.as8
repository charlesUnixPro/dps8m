	tsx0	log2		" calculate log2 (x)
	dfmp	log_10_of_2	" EAQ := log_10_of_2 * log2 (x)
	frd     0
	"tra     pr3|0		" return to caller

    return  log_base_10_

log_base_2_:
    save    0,1,3
    FLD     2, 1*               " X = ARGUMENT

	tsx0	log2		" calculate log2 (x)
	frd     0
"	tra     pr3|0		" return to caller

    return  log_base_2_

log_base_e_:
    save    0,1,3
    FLD     2, 1*               " X = ARGUMENT

	tsx0	log2		" calculate log2 (x)
	dfmp	log_e_of_2	" EAQ := log_e_of_2 * log2 (x)
	frd     0
	"tra     pr3|0		" return to caller

