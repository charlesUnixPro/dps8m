	fmp     z
	dfad	Ep4
	fmp     z
	dfad	Ep3
	fmp     z
	dfad	Ep2
	fmp     z
	dfad	Ep1
	fmp     z
	dfad	Ep0

	ade     iy		" addr (result) -> expon = addr (result) -> expon + iy
	"tra     pr3|0		" return result in EAQ
