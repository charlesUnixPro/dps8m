	ufa     =7b25,du	" AQ := 8/floor(y+1),64/fraction part of y
	sta     iy
	ora     =o776000,du	" AQ := 8/-1,64/fraction part of y
	lde     =7b25,du	" EAQ := ry = unnormalized y - floor(y+1)
	fad     =0.0,du		" EAQ := ry = normalized y - floor(y+1)
                        " result = part_exp2 (ry)

