de dsbr.u 1
de dsbr.addr 020000
de ppr.ic 0
de ppr.prr 0
de ppr.psr 0
de ppr.p 1
dpsinit segment table
dpsinit SDWAM
load Play/PlayA.oct segment 0 address  0
load Play/UtilsA.oct segment 1 address  010000
;set debug stderr
;set cpu debug=trace;appending;regdumpaqi;regdumpppr;addrmod
;set cpu debug=trace
dpsdump segment table
g 0

